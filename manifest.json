// manifest.json
{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "Interact with my website",
  "permissions": [
    "tabs",
     "activeTab",
    "scripting"
   
  ],

   "content_scripts": [
    {
      "matches": ["http://les-professionnels-de-madagascar.com/category/*"],
      
      "js": ["contentScript.js"]
    }
  ],
  

"background": {
    "service_worker": "background.js"

  },

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon32.png",
      "128": "images/icon128.png"
    }
  }
  
}


 

